<?xml version='1.0' encoding='utf-8'?>
<widget id="marinenko.trianglemobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>.</name>
    <description>
       Brings the Web to android.
    </description>
    <author email="misha@marinenko.rf.gd" href="http://marinenko.rf.gd">
        Misha Marinenko
    </author>
    <content src="dist/index.html" />
    <access origin="*" />
    <allow-intent href="http://*" />
    <allow-intent href="https://*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
